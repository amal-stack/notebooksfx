<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<HBox xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.amalstack.notebooksfx.editor.controller.StatusBarController"
      xmlns="http://javafx.com/javafx/18" id="HBox" alignment="CENTER_LEFT"
      spacing="5.0" VBox.vgrow="NEVER">
    <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
    </padding>
    <HBox fx:id="breadCrumbParent" HBox.hgrow="ALWAYS"/>
    <Pane HBox.hgrow="ALWAYS"/>
    <Label fx:id="editorRowPositionLabel" HBox.hgrow="NEVER"/>
    <Label text=" " HBox.hgrow="NEVER"/>
    <Label fx:id="editorColumnPositionLabel" HBox.hgrow="NEVER"/>
</HBox>